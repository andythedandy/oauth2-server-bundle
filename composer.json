{
    "name": "bshaffer/oauth2-server-bundle",
    "type": "symfony-bundle",
    "description": "Symfony OAuth2ServerBundle",
    "keywords": ["oauth", "oauth2", "security"],
    "homepage": "http://github.com/bshaffer/oauth2-server-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Brent Shaffer",
            "email": "bshafs@gmail.com"
        }
    ],
    "minimum-stability":"dev",
    "require": {
        "php": ">=5.3",
        "wirelane/oauth2-server-php": "dev-fix_set-status-code",
        "wirelane/oauth2-server-httpfoundation-bridge": "dev-fix_set-status-code"
    },
    "require-dev": {
        "doctrine/orm": "~2.4,>=2.4.5"
    },
    "autoload": {
        "psr-0": { "OAuth2\\ServerBundle": "" }
    },
    "target-dir": "OAuth2/ServerBundle",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/wirelane/oauth2-server-php.git"
        },
        {
            "type": "git",
            "url": "https://github.com/wirelane/oauth2-server-httpfoundation-bridge.git"
        }
    ]
}
